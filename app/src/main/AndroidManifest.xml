<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rohit.simpleui2" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".TestActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".CreditCardDetails"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".WalletGatewayDetailsPayU"
            android:label="@string/title_activity_wallet_gateway_details_pay_u"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".PaymentInterface"
            android:label="@string/title_activity_payment_interface"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FinalPaymentStep"
            android:label="@string/title_activity_final_payment_step"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".FinalPaymentStepForSavedCard"
            android:label="@string/title_activity_final_payment_step"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
